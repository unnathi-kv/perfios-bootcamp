package com.test;
class Car
{
	public int carid;
	public String carname;
	String city;
	public Car()
	{
		System.out.println("Car Id: "+carid);
		System.out.println("Car name: "+carname);
		System.out.println("Car city: "+city);	
	}
	public Car(int x, String y, String z)
	{
		System.out.println("Car Id: "+x);
		System.out.println("Car name: "+y);
		System.out.println("Car city: "+z);

	}
	public Car(Car obj)
	{
		System.out.println("Car city: "+city);
	}

}
public class Eg6 {

	public static void main(String[] args)
	{
		Car obj = new Car();
		Car obj1 = new Car(12,"BMW","Germany");
		Car obj2 = new Car(obj);
		
	}
}
