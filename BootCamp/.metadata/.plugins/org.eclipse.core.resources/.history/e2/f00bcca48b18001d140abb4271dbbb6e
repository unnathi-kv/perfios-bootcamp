package com.test;
//Singleton Design Pattern.(doubt)
class SingleTon
{
	static SingleTon obj= null;
	private SingleTon()
	{
		
	}
	public static SingleTon getObj()
	{
		if(obj==null)
		{
			obj=new SingleTon();
		}
		return obj;
		
	}
}
public class Eg7 {
 public static void main(String[] args)
 {
	 SingleTon obj1 = SingleTon.getObj();
	 SingleTon obj2 = SingleTon.getObj();
	 
 }
}
