package com.test;

public class Eg1 {
	int a = 10; //non-static - stores in heap memory
	static int b = 20;//static- stores in stack memory
	public void disp()
	{
		System.out.println(a+ " "+Eg1.b);
	}
	public static void main(String[] args)
	{
		Eg1 obj =new Eg1();
		Eg1 obj1 = new Eg1();
		obj=obj1;
		obj.disp();
		System.out.println(obj);// prints hashcode(object reference - generated by JVM) of object
		System.out.println(obj1.hashCode());//hashCode()- method in object class which prints hashcode of object and returns integer.
		System.out.println(obj.hashCode());
	}
	
} 
