package p1;

class DataStd {
	public int id =01;
	public int roll = 1;
	public String name = "Aditya";
	public String company= "Perfios";
	public int score = 79;
	
	}
public class ValidateData extends DataStd
{
	public void disp()
	{
		if(score >=60 && score<70) {
			System.out.println("ViT");
		}
		else if(score >=70 && score<80) {
			System.out.println("NiT");
		}
	}
}
