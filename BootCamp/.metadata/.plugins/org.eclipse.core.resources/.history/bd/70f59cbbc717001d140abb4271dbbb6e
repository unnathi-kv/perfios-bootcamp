package com.perfios;

public class Example1 {
	
	private int x =10;
	int y = 20;
	protected int z = 30;
	public int a = 40;
	public void disp()
	{
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
		System.out.println(a);
		
	}

}
class Exp2 extends Example1{
	public void disp()
	{
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
		System.out.println(a);
	}
	
	
}


