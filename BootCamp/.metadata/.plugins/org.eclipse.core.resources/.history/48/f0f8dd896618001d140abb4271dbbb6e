package p1;

class DataStd {
	public int id;
	public int roll;
	public String name;
	public String company;
	public int score;
	
	}
public class ValidateData extends DataStd
{
	public void disp()
	{
		if(score >=60 && score<70) {
			System.out.println("ViT");
		}
		else if(score >=70 && score<80) {
			System.out.println("NiT");
		}
	}
}
